// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Postings {
    id String @id @default(cuid())
    employer String
    employerImg String
    location String
    title String
    type String
    level String
    salary String
    description String
    createdAt DateTime @default(now())
    skills String
}